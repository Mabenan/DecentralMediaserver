{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Launch Chrome against localhost",
            "port": 9222,
            "webRoot": "${workspaceFolder}/dist/electron-angular",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${workspaceFolder}/src/*",                // Example: "meteor://ðŸ’»app/main.ts" -> "/Users/me/project/main.ts"
            }
        }
    ]
}